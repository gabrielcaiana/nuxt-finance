{
  "private": true,
  "name": "nuxt-finance",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "format-check": "npx prettier --check ./",
    "format-write": "npx prettier --write ./",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "pscale:dev": "pscale connect nuxt-finance dev --port 3309",
    "pscale:main": "pscale connect nuxt-finance main --port 3309",
    "pscale:shell": "pscale shell nuxt-finance dev",
    "pscale:update": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "server": "ts-node-esm --experimental-specifier-resolution=node api/src/server.ts"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.2",
    "@pinia/nuxt": "^0.4.2",
    "@vitest/ui": "^0.22.1",
    "flowbite": "^1.5.3",
    "mitt": "^3.0.0",
    "vitest": "^0.23.4",
    "@prisma/client": "^4.3.1",
    "cors": "^2.8.5",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^9.0.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/test-utils": "^2.0.2",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jsdom": "^20.0.0",
    "nuxt": "3.0.0-rc.11",
    "nuxt-icon": "^0.1.7",
    "postcss": "^8.4.16",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "prisma": "^4.3.1",
    "ts-node-dev": "^2.0.0"
  }
}
